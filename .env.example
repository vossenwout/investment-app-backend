# Supabase local development credentials
SUPABASE_PROJECT_ID=investment-app-backend
SUPABASE_ANON_KEY=replace-with-local-anon-key
SUPABASE_SERVICE_ROLE_KEY=replace-with-local-service-role-key
SUPABASE_URL=http://localhost:54321

# Edge runtime ignores env vars starting with SUPABASE_, so provide aliases.
API_URL=http://127.0.0.1:54321
EDGE_FUNCTION_API_URL=http://host.docker.internal:54321
SERVICE_ROLE_KEY=replace-with-local-service-role-key

# Connection string that tests can use (user:pass@host:port/db?params)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres
